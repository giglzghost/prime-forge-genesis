# General app settings
NODE_ENV=development
PORT=4000

# LLM / AI provider (you will fill these in your local .env, not in git)
LLM_PROVIDER=openai            # e.g. openai, anthropic, etc.
LLM_MODEL=gpt-4.1-mini         # or another model you choose
LLM_API_KEY=your_llm_api_key_here

# Git / repository access (optional for now)
GIT_DEFAULT_REPO_URL=
GIT_ACCESS_TOKEN=

# Cloud provider config (Azure / Vercel / other â€“ fill in per environment)
CLOUD_PROVIDER=
CLOUD_API_KEY=
CLOUD_SUBSCRIPTION_ID=
CLOUD_RESOURCE_GROUP=

# Payments / NFT (future: PayPal, marketplaces, etc.)
PAYMENTS_PROVIDER=
PAYMENTS_API_KEY=

# Self-edit protection: set a secret token in your real .env before enabling
SELF_EDIT_TOKEN=change_me_before_use
